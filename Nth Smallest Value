#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
		int a[10];
		for(int i=0;i<10;i++)
		{
			cin>>a[i];
		}
		int min;
		for(int l1=0;l1<9;l1++)
		{
			for(int l2=l1+1;l2<10;l2++)
			{
				if(a[l1]>a[l2])
				{
					int temp=a[l1];
					a[l1]=a[l2];
					a[l2]=temp;
				}
			}
		}
		cout<<a[1]<<endl;
	}
	return 0;
}
